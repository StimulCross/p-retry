{
	"compilerOptions": {
		"composite": true,
		"target": "ES2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"lib": ["ES2022"],
		"declaration": true,
		"declarationMap": true,
		"inlineSourceMap": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"useUnknownInCatchVariables": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true
	}
}
